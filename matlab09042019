%I*R=U
%I1(R1+R2+R3)- I2(R2) +I3(0) = E1
%-I1(R3)+ I2(R2+R4+R5)- I3(R5) = -E2
%I1(0)- I2(R5) +I3(R6+R5+R7) = -E3
r=[1 2 3 6 8 9 3];
t=0:0.01:5;
E1=sin(2*pi*2*t);
E2=-cos(t);
E3 =1.5*ones(size(t));

R=[ sum(r(1:3)), -r(2), 0 ;
  -r(2), sum(r([2,4,5])), -r(5);
  0, -r(5), sum(r([6,5,7]))
  ];

Ein=[E1; -E2; -E3];

I = R\Ein;
%Ur5=r(5)*(I(2)-I(3));
Ur5=r(5)*(I(2,:)-I(3,:));
%Pr5=Ur5*(I(2)-I(3));
Pr5=Ur5.*(I(2,:)-I(3,:));
Ksp = r(4)*I(2,:)+Ur5+E2+r(2)*(I(2,:)-I(1,:));

